msgid "All Applications"
msgstr "Minden alkalmazás"

#: ../src/appfinder-model.c:540
msgid "Name"
msgstr "Név"

#: ../src/appfinder-model.c:541
msgid "Comment"
msgstr "Megjegyzés"

#: ../src/appfinder-model.c:542
msgid "Command"
msgstr "Parancs"

#: ../src/appfinder-model.c:543
msgid "Categories"
msgstr "Kategóriák"

